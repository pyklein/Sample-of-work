ul#tabnav {
    font: bold 11px verdana, arial, sans-serif;
    list-style-type: none;
    padding-bottom: 24px;
    border-bottom: 1px solid #B4AF91;
    border-top:none;
    margin: 0;
}

ul#tabnav li {
    float: left;
    height: 21px;
    background-color: #EAEAEA;
    margin: 2px 2px 0 2px;
    border: 1px solid #B4AF91;
}

ul#tabnav li.active {
    border-bottom: 1px solid #B4AF91;
    background-color: #EAEAEA;
}

ul#tabnav li.active a {
    color: white;
    background-color: #EAEAEA;
}

#tabnav a {
    display: block;
    color: black;
    background-color: #EAEAEA;
    text-decoration: none;
    padding: 4px;
}

#tabnav a:hover {
    background: #EFECCA;
}

